{
    "gh": {
        "args": ["<command>"],
        "workspace": [
            "Repository at /workspace/<project-name>/",
            "All gh commands should be run from repository root"
        ],
        "environment": [
            "/workspace/shared/configs/access_tokens/github.txt",
            "/workspace/shared/configs/.ssh"
            "https://github.com/ywatanabe1989/<project-name>",
        ],
        "note": [
            "Ensure .gitignore is configured",
        ],
        "example": "(call-process \"gh login --with-token < /workspace/shared/configs/access_tokens/github.txt\")",
        "version": "1.0.0",
    }
}